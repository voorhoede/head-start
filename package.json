{
  "name": "head-start",
  "type": "module",
  "version": "0.0.1",
  "workspaces": [
    "config/datocms"
  ],
  "license": "ISC",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "astro": "astro",
    "prebuild": "run-s prep",
    "build": "astro build",
    "postbuild": "npm run post",
    "cloudflare:build": "jiti scripts/build.ts",
    "create": "plop --plopfile config/plop/plopfile.mjs",
    "create:api": "plop --plopfile config/plop/plopfile.mjs api",
    "create:block": "plop --plopfile config/plop/plopfile.mjs block",
    "create:component": "plop --plopfile config/plop/plopfile.mjs component",
    "create:page": "plop --plopfile config/plop/plopfile.mjs page",
    "predev": "run-s prep",
    "dev": "run-p dev:* --print-label",
    "dev:astro": "astro dev",
    "dev:graphql": "chokidar 'src/**/*.graphql' -c 'npm run prep:datocms-types'",
    "dev:icons": "chokidar 'src/assets/icons/*.svg' -c 'npm run prep:icons'",
    "dev:translations": "jiti scripts/watch-translations.ts",
    "prep": "run-s prep:* --print-label",
    "prep:clean-astro-env": "rm -rf node_modules/.astro",
    "prep:cloudflare-env": "touch .dev.vars",
    "prep:datocms-types": "graphql-codegen --config .graphqlrc.ts",
    "prep:download-file-proxy-map": "jiti scripts/download-file-proxy-map.ts",
    "prep:download-redirects": "jiti scripts/download-redirects.ts",
    "prep:download-site-data": "jiti scripts/download-site-data.ts",
    "prep:download-translations": "jiti scripts/download-translations.ts",
    "prep:download-item-types": "jiti scripts/download-item-types.ts",
    "prep:icons": "svg-sprite --svg-doctype=false --symbol --symbol-dest='src/assets' --symbol-sprite=icon-sprite.svg src/assets/icons/*.svg && jiti scripts/icon-types.ts",
    "preview": "wrangler pages dev ./dist",
    "lint": "run-s lint:* --print-label",
    "lint:astro": "astro check",
    "lint:eslint": "eslint .",
    "lint:html": "html-validate --config config/htmlvalidate/config.json dist/",
    "pretest": "npm run prep",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "post": "run-s post:* --print-label",
    "post:remove-public-dir-readme": "rm dist/README.md",
    "postinstall": "npx husky install",
    "seed:docs": "npm run seed:docs --workspace=config/datocms",
    "cms:manage": "jiti scripts/cms-manage.ts",
    "cms:environments:create": "jiti scripts/cms-create-environment.ts",
    "cms:environments:destroy": "jiti scripts/cms-destroy-environment.ts",
    "cms:environments:sync": "jiti scripts/cms-sync-environment.ts",
    "cms:environments:promote": "jiti scripts/cms-promote-environment.ts",
    "cms:migrations:generate": "jiti scripts/cms-generate-migration.ts",
    "cms:upload-block-previews": "jiti scripts/upload-block-previews.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.6.7",
    "@astrojs/sitemap": "^3.5.1",
    "@fontsource/archivo": "^5.1.0",
    "@inquirer/prompts": "^7.5.3",
    "@nanostores/persistent": "^0.10.1",
    "@rollup/plugin-graphql": "^2.0.3",
    "accept-language-parser": "^1.5.0",
    "astro": "^5.13.5",
    "async": "^3.2.6",
    "datocms-listen": "^0.1.15",
    "datocms-structured-text-utils": "^4.0.1",
    "get-video-id": "^3.6.5",
    "globby": "^13.2.2",
    "hls.js": "^1.5.2",
    "html-validate": "^9.2.0",
    "jiti": "^1.20.0",
    "nanostores": "^0.9.5",
    "oembed-providers": "^1.0.20230906",
    "pretty-bytes": "^6.1.1",
    "promise-all-props": "^3.0.0",
    "regexparam": "^3.0.0",
    "rosetta": "^1.1.0",
    "scule": "^1.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.4",
    "@datocms/cli": "^3.0.1",
    "@datocms/cma-client-node": "^3.4.0",
    "@eslint/js": "^9.31.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.11",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/accept-language-parser": "^1.5.6",
    "@types/async": "^3.2.25",
    "@types/dotenv-safe": "^8.1.4",
    "@types/eventsource": "^1.1.15",
    "@types/jsdom": "^21.1.7",
    "chokidar-cli": "^3.0.0",
    "datocms-listen": "^0.1.15",
    "dotenv-safe": "^8.2.0",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-depend": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eventsource": "^2.0.2",
    "globals": "^16.3.0",
    "husky": "^8.0.3",
    "jsdom": "^25.0.0",
    "msw": "^2.4.2",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "proper-tags": "^2.0.2",
    "robots-parser": "^3.0.1",
    "sonda": "^0.7.1",
    "svg-sprite": "^2.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vitest": "^3.0.5",
    "wrangler": "^4.34.0"
  },
  "nano-staged": {
    "*.{astro,js,ts}": "npm run lint:eslint -- --fix --max-warnings 0"
  }
}
