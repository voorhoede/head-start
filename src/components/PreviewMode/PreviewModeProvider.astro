---
import PreviewMode from './PreviewMode.astro';
import PreviewModeLogin from './PreviewModeLogin.astro';
const { showPreviewBar, isPreviewAuthOk } = Astro.locals;
---
{
  showPreviewBar
    ? isPreviewAuthOk
      ? <PreviewMode><slot /></PreviewMode>
      : <PreviewModeLogin />
    : <slot />
}
